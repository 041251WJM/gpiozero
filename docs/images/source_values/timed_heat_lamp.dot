/* vim: set et sw=4 sts=4: */

digraph {
    graph [rankdir=RL];
    edge [arrowhead=normal, style=solid];

    /* Devices */
    node [shape=rect, style=filled, color="#2980b9", fontname=Sans, fontcolor="#ffffff", fontsize=10];

    lamp [label="Lamp"]
    daytime [label="Daytime"]

    daytime -> lamp;
}
